  "devDependencies": {
    "@bigcommerce/eslint-config": "^2.10.0",
    "@bigcommerce/eslint-config-catalyst": "workspace:^",
    "@types/node": "^20.17.22",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
