{
  "name": "@bigcommerce/reactant",
  "description": "BigCommerce components for Catalyst.",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./postcss-config": "./postcss.config.js",
    "./tailwind-config": "./tailwind.config.js",
    "./cn": {
      "import": "./dist/lib/utils.mjs",
      "require": "./dist/lib/utils.js",
      "types": "./dist/lib/utils.d.ts"
    },
    "./*": {
      "import": "./dist/components/*/index.mjs",
      "require": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js,.cjs --max-warnings 0",
    "test": "jest"
  },
  "files": [
    "dist",
    "postcss.config.js",
    "tailwind.config.js"
  ],
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "embla-carousel-react": "8.0.0-rc14",
    "focus-trap-react": "^10.2.3",
    "lucide-react": "^0.292.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-radix": "^2.8.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.4"
  },
  "devDependencies": {
    "@bigcommerce/catalyst-configs": "workspace:^",
    "@bigcommerce/eslint-config-catalyst": "workspace:^",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^6.7.0",
    "typescript": "^5.2.2"
  }
}
