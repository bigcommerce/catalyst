{
  "name": "@bigcommerce/reactant",
  "description": "BigCommerce components for Catalyst.",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./postcss-config": "./postcss.config.js",
    "./tailwind-config": "./tailwind.config.js",
    "./cs": {
      "import": "./dist/utils/cs.mjs",
      "require": "./dist/utils/cs.js",
      "types": "./dist/utils/cs.d.ts"
    },
    "./*": {
      "import": "./dist/components/*/index.mjs",
      "require": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js,.cjs --max-warnings 0"
  },
  "files": [
    "dist",
    "postcss.config.js",
    "tailwind.config.js"
  ],
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "focus-trap-react": "^10.2.1",
    "lucide-react": "^0.216.0",
    "tailwind-merge": "^1.12.0",
    "tailwindcss-radix": "^2.8.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.4"
  },
  "devDependencies": {
    "@bigcommerce/catalyst-configs": "workspace:^",
    "@bigcommerce/eslint-config-catalyst": "workspace:^",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  }
}
