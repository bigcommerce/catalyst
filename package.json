    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "dotenv-cli": "^7.4.2",
    "turbo": "^2.0.6",
    "typescript": "^5.5.3"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
